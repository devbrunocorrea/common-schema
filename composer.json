{
    "name": "gpupo/common-schema",
    "description": "Common schema",
    "homepage": "https://opensource.gpupo.com/common-schema/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["sphinx", "schema", "xml", "xmlpipe2", "order"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
          "Gpupo\\CommonSchema\\": "src",
          "Gpupo\\CommonSchema\\Build\\": "build"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\CommonSchema\\Tests\\": "tests",
            "App\\": "var/src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3"
    },
    "bin": ["bin/common-schema"],
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": "^7.3",
        "gpupo/common": "^4.6|dev-master",
        "symfony/var-dumper": "^4.2.2",
        "gpupo/common-sdk": "^4.5|dev-master",
        "doctrine/orm": "^2.6",
        "gedmo/doctrine-extensions": "^2.4",
        "symfony/yaml": "^4.2.2",
        "symfony/dotenv": "^4.2.2",
        "symfony/console": "^4.2.2",
        "symfony/filesystem": "^4.2.2",
        "symfony/finder": "^4.2.2"
    }
}
